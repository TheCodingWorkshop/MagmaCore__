{
    "name": "magmacore/magmacore",
    "description": "A Small modular PHP framework Build for web applications",
    "keywords": [
        "php",
        "framework",
        "magmacore",
        "lavastudio"
    ],
    "type": "library",
    "license": "MIT",
    "time": "2020-12-25",
    "readme": "https://github.com/TheCodingWorkshop/MagmaCore__/blob/main/README.md",
    "support": {
        "email": "lavastudio.v1@gmail.com",
        "source": "https://github.com/TheCodingWorkshop/MagmaCore__.git",
        "wiki": "https://github.com/TheCodingWorkshop/MagmaCore__/wiki",
        "issues": "https://github.com/TheCodingWorkshop/MagmaCore__/issues"
    },
    "homepage": "https://thecodingworkshop.github.io/MagmaCore__/",
    "authors": [
        {
            "name": "TheCodingWorkshop",
            "email": "riccodingworkshop@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "beta",
    "require": {
        "ext-mbstring": "*",
        "ext-iconv": "*",
        "ext-pdo": "*",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-imap": "*",
        "ext-simplexml": "*",
        "symfony/yaml": "^5.2",
        "symfony/http-foundation": "^5.2",
        "paragonie/anti-csrf": "^2.3",
        "symfony/dotenv": "^5.2",
        "flatbase/flatbase": "^0.4.1",
        "symfony/console": "^5.3",
        "phpmailer/phpmailer": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "MagmaCore\\": "src/"
        },
        "files": []
    },
    "autoload-dev": {
        "psr-4": {
            "MagmaCore\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "mockery/mockery": "^1.4"
    },
    "suggest": {
        "paragonise/anti-csrf": "Added for csrf form protection"
    }
}
